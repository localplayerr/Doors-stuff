local config = loadstring(game:HttpGet("https://raw.githubusercontent.com/localplayerr/Doors-stuff/refs/heads/main/Custom%20badges%20in%20lobby/Source"))()

config.createbadge({
  title = "Example badge", -- Title here
  description = "Very cool description", -- description here
  reason = "Run this script", -- reason here
  logo = "http://www.roblox.com/asset/?id=5430597512", -- logo here (should be rbxassetid)
  color = ColorSequence.new{
  ColorSequenceKeypoint.new(0, Color3.fromRGB(134, 255, 121)),
  ColorSequenceKeypoint.new(1, Color3.fromRGB(112, 241, 255))
  }, -- colors
  category = "General", -- General,Entities,Crucifix,Floors
  prize = true, -- rnable/disable prize
  knobs = 500, -- knobs that will be given 
  revives = 3 -- revives that will be given
})
